</span><span><span class="hljs-keyword">const</span></span><span> carousel = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'carouselTrack'</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> nextBtn = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">querySelector</span></span><span>(</span><span><span class="hljs-string">'.carousel-btn.next'</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> prevBtn = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">querySelector</span></span><span>(</span><span><span class="hljs-string">'.carousel-btn.prev'</span></span><span>);

</span><span><span class="hljs-comment">// Calculate scroll amount based on image width + gap</span></span><span>
</span><span><span class="hljs-keyword">const</span></span><span> image = carousel.</span><span><span class="hljs-title function_">querySelector</span></span><span>(</span><span><span class="hljs-string">'.carousel-img'</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> gap = </span><span><span class="hljs-number">16</span></span><span>; </span><span><span class="hljs-comment">// Same as your CSS gap</span></span><span>
</span><span><span class="hljs-keyword">const</span></span><span> scrollAmount = image.</span><span><span class="hljs-property">offsetWidth</span></span><span> + gap;

nextBtn.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">'click'</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
  carousel.</span><span><span class="hljs-title function_">scrollBy</span></span><span>({ </span><span><span class="hljs-attr">left</span></span><span>: scrollAmount, </span><span><span class="hljs-attr">behavior</span></span><span>: </span><span><span class="hljs-string">'smooth'</span></span><span> });
});

prevBtn.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">'click'</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
  carousel.</span><span><span class="hljs-title function_">scrollBy</span></span><span>({ </span><span><span class="hljs-attr">left</span></span><span>: -scrollAmount, </span><span><span class="hljs-attr">behavior</span></span><span>: </span><span><span class="hljs-string">'smooth'</span></span><span> });
});
</span></span></code></div></div></pre>
<p data-start="1380" data-end="1544">This version dynamically uses the width of your actual images plus the gap between them  so it will <strong data-start="1481" data-end="1509">scroll exactly one photo</strong> at a time, no more jumpy behavior.</p>
