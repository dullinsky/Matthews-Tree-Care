  </span><span><span class="hljs-keyword">const</span></span><span> prevArrow = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"prevArrow"</span></span><span>);
  </span><span><span class="hljs-keyword">const</span></span><span> nextArrow = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"nextArrow"</span></span><span>);

  </span><span><span class="hljs-keyword">let</span></span><span> currentImageIndex = </span><span><span class="hljs-number">0</span></span><span>;



  </span><span><span class="hljs-comment">// ------------------------------</span></span><span>
  </span><span><span class="hljs-comment">// Open Fullscreen Viewer</span></span><span>
  </span><span><span class="hljs-comment">// ------------------------------</span></span><span>
