      </span><span><span class="hljs-attr">method</span></span><span>: </span><span><span class="hljs-string">'POST'</span></span><span>,
      </span><span><span class="hljs-attr">body</span></span><span>: formData,
      </span><span><span class="hljs-attr">headers</span></span><span>: {
        </span><span><span class="hljs-title class_">Accept</span></span><span>: </span><span><span class="hljs-string">'application/json'</span></span><span>
      }
    });

    </span><span><span class="hljs-keyword">if</span></span><span> (response.</span><span><span class="hljs-property">ok</span></span><span>) {
      form.</span><span><span class="hljs-title function_">reset</span></span><span>();
      thankYou.</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">display</span></span><span> = </span><span><span class="hljs-string">'block'</span></span><span>;

      </span><span><span class="hljs-comment">// Wait 3 seconds, then redirect</span></span><span>
      </span><span><span class="hljs-built_in">setTimeout</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {
        </span><span><span class="hljs-variable language_">window</span></span><span>.</span><span><span class="hljs-property">location</span></span><span>.</span><span><span class="hljs-property">href</span></span><span> = </span><span><span class="hljs-string">'/'</span></span><span>;
      }, </span><span><span class="hljs-number">3000</span></span><span>);
    } </span><span><span class="hljs-keyword">else</span></span><span> {
      </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">'Oops! There was a problem sending your message.'</span></span><span>);
    }
  } </span><span><span class="hljs-keyword">catch</span></span><span> (error) {
    </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">'There was an error sending the form.'</span></span><span>);
  }
});
</span></span></code></div></div></pre>
